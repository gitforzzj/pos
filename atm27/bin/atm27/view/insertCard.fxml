<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="bigBP" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="atm27.controller.InsertCardController">
   <bottom>
      <HBox prefHeight="165.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="card">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="insertCash" alignment="BOTTOM_RIGHT" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#insertCash" text="插卡" textAlignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <padding>
                        <Insets bottom="3.0" left="20.0" right="20.0" top="3.0" />
                     </padding>
                     <cursor>
                        <Cursor fx:constant="NONE" />
                     </cursor>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="吐钞口:">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="spitOutput" prefHeight="200.0" prefWidth="200.0" />
               </children>
               <opaqueInsets>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </opaqueInsets>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="打印凭证:">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="printOutput" prefHeight="200.0" prefWidth="200.0" />
               </children>
               <padding>
                  <Insets bottom="15.0" right="15.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </bottom>
   <center>
      <BorderPane fx:id="insertBP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <center>
            <Label text="欢迎使用ATM系统，请插卡" BorderPane.alignment="CENTER">
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
   </center>
</BorderPane>
